{
  "name": "responsive-img-material-ui",
  "version": "1.0.2",
  "description": "A material-ui component which display an image from different sources based on the page's current width (based on material-ui defined breakpoints).",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "deploy": "cross-env NODE_ENV=production npm run build && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iulian-radu-at/img-material-ui.git"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "keywords": [
    "img",
    "material-ui",
    "img-material-ui",
    "responsive-img-material-ui",
    "react",
    "react-component",
    "ui",
    "material design"
  ],
  "author": "Iulian Radu <iulian.radu@gmx.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iulian-radu-at/img-material-ui/issues"
  },
  "homepage": "https://github.com/iulian-radu-at/img-material-ui#readme",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@types/babel-core": "^6.25.6",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/react-transition-group": "^2.9.0",
    "@types/rimraf": "^2.0.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.7.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.4"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules",
      "./history"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testRegex": "src/*(/(__tests?__/)([^_].*/)*?[^_][^/]*?\\.(test|spec)?\\.(js?))$",
    "setupFiles": [
      "./enzymeAdapter.setup.js"
    ]
  }
}
